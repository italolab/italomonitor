[ req ]
default_bits       = 2048
prompt             = no
default_md         = sha256
distinguished_name = dn
x509_extensions    = v3_req

[ dn ]
C  = BR
ST = Pernambuco
L  = Sao Jose do Egito
O  = Italo Monitor
CN = italomonitor.com.br

[ v3_req ]
subjectAltName = @alt_names

[ alt_names ]
DNS.1 = italomonitor.com.br
DNS.2 = www.italomonitor.com.br
DNS.3 = localhost
IP.1  = 127.0.0.1